<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd
        ">
 	<!-- 使用dubbo协议暴露服务 -->
 	
 	<!-- 应用版本信息查询的dubbo -->
    <bean id="sysUserClient" class="com.haier.isales.app.service.impl.SysUserClientImpl">
   		 <property name="sysUserService" ref="sysUserService" />
   		 <property name="sysUserCollectService" ref="sysUserCollectService" />
    </bean>  
    <dubbo:service interface="com.haier.isales.app.service.SysUserClient" owner="isales" ref="sysUserClient" version="${project.version}"  protocol="dubbo" delay="-1" registry="isalesRegistry"/>

</beans>